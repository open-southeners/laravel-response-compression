{
    "name": "open-southeners/laravel-vapor-response-compression",
    "description": "Add server-side response compression with different algorithm support (Gzip, brotli...)",
    "license": "MIT",
    "keywords": [
        "open-southeners",
        "vapor",
        "laravel",
        "response-compression",
        "brotli"
    ],
    "authors": [
        {
            "name": "Ruben Robles",
            "email": "d8vjork@outlook.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "ext-zlib": "^8.0|^8.1|^8.2"
    },
    "require-dev": {
        "nunomaduro/larastan": "^2.0",
        "orchestra/testbench": "^7.0|^8.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "OpenSoutheners\\LaravelVaporResponseCompression\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenSoutheners\\LaravelVaporResponseCompression\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenSoutheners\\LaravelVaporResponseCompression\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}